digraph mtnuniverse {
     subgraph libraries {
          label = "Libraries";
          color=blue;

          image    [label = "mayton.libs:image:1.4.1"];
          math     [label = "mayton.libs:math:1.0-SNAPSHOT" color = red];
          neural   [label = "mayton.libs:neural:?" color = red];
          "graph"  [label = "mayton.libs:mtn-graph:5.0.4" ];
